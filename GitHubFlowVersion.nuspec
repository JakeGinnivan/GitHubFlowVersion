<?xml version="1.0"?>
<package >
  <metadata>
    <id>GitHubFlowVersion</id>
    <version>$version$</version>
    <authors>JakeGinnivan</authors>
    <owners>JakeGinnivan</owners>
    <licenseUrl>https://github.com/JakeGinnivan/GitHubFlowVersion/blob/master/LICENSE</licenseUrl>
